/* example 081 â€“ a square of stars  */
#include <stdio.h>

int main (void)
{
    int i, j;

    for (i = 1; i <= 20; i++) {			// create 20 rows
        for (j = 1; j <= 20; j++) {		// create 20 stars
           if ((j == i) || (j == 20 - i + 1))  	// condition to create a blank
	      printf(" ");  			// print out a blank
           else
	      printf("*"); 			// else print a star
        }
        printf("\n");  				// next line
    }
    return 0;
}
