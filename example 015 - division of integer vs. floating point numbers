/* example 015 - division of integer vs. floating point numbers */
#include <stdio.h>

int main(void)
{
   double a, b, c, d, e;

   a = 5/9;
   b = 5.0/9;
   c = 5.0/9.0;
   d = 5/2;
   e = 8/3;

   printf("a = %f\n",a);   // a = 0.000000
   printf("b = %f\n",b);   // b = 0.555556
   printf("c = %f\n",c);   // c = 0.555556
   printf("d = %f\n",d);   // d = 2.000000
   printf("e = %f\n",e);   // e = 2.000000
   return 0;
}
