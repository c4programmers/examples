/* example 098 â€“ call by value */
#include <stdio.h>

void swap(int x, int y) { 	// Swap function that swaps 2 values
    int temp;

    temp = x;   		// temp = 5
    x = y;      		// x = 8
    y = temp;   		// y = 5

    printf("x = %d, y = %d\n", x, y);
}

int main()
{
    int a = 5, b = 8;
    printf("a = %d, b = %d\n", a, b);
    swap(a, b); 		// Pass by value
    printf("a = %d, b = %d\n", a, b);

    return 0;
}
