/* example 008 â€“ accuracy float vs. double */
#include <stdio.h>

int main()
{
   float fa = 1.123, fb = -4.0000000, fc = 399999.0;
   double da = 1.123, db = -4.0000000, dc = 399999;
   float result_f = fa/fb*fc;
   double result_d = da/db*dc;

   printf("result float 1.123 / - 4.0000000 * 3.9999999 = \t%f\n", result_f);
   printf("result double 1.123 / - 4.0000000 * 3.9999999 = \t%lf\n", result_d);
   return 0;
}
