/* example 067 â€“ nested if-else statements */
#include <stdio.h>

int main(void)
{
   int voltage;

   printf("Value of voltage? ");
   scanf("%i",&voltage);

   if (voltage > 5)
      if (voltage > 50)
         if (voltage > 1000)
	    printf("very high voltage ");
	 else
	    printf("high voltage");
      else
         printf("low voltage");
   else
      printf("very low voltage");

   return 0;
}
