/* example 099 â€“ call by reference */
#include <stdio.h>

void swap(int *x, int *y) { // y and y are pointers
    int temp;

    temp = *x;   	// temp = 5
    *x = *y;      	// x = 8
    *y = temp;   	// y = 5

    printf("x = %d, y = %d\n", *x, *y);
}

int main()
{
    int a = 5, b = 8;

    printf("a = %d, b = %d\n", a, b);
    swap(&a, &b);   	// Pass by reference
    printf("a = %d, b = %d\n", a, b);

    return 0;
}
