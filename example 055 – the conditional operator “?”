/* example 055 – the conditional operator “?” */
#include <stdio.h>

int main (void)
{
   int num1, num2, biggerNum;

   printf("Please enter two numbers separated by spaces: ");
   scanf("%i %i",&num1,&num2); // enter two numbers

   biggerNum = (num1 > num2) ?num1 :num2;

   printf("\n%i is the bigger number",biggerNum);

   return 0;
}
