/* example 207 â€“ structures with arrays */
#include <stdio.h>

struct data {
    char	 firstlastName[20];
    char lastName[20];
    int employeeNumber;
} employee;

int main(void)
{
    printf("Enter the first and last name separated by a space: ");
    scanf("%s %s", employee.firstlastName, employee.lastName);

    printf("Enter your employee number: ");
    scanf("%i", &employee.employeeNumber);

    printf("%s %s, - Nr.:%d.\n",employee.firstlastName, employee.lastName,
    employee.employeeNumber);

    return 0;
}
