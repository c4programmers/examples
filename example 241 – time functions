/* example 241 â€“ time functions */
#include <stdio.h>
#include <time.h>

int main() 
{
    time_t start_time, end_time;
    double elapsed_time;

    start_time = time(NULL);
    // Code to be timed here
    end_time = time(NULL);

    elapsed_time = difftime(end_time, start_time);

    printf("Elapsed time: %f seconds\n", elapsed_time);

    return 0;
}
