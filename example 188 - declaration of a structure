/* example 188 - declaration of a structure */
#include <stdio.h>

typedef struct {	    // declaration of structure numbers with typedef
   int num1;        // member num1
   int num2;        // member num2
}numbers;

int main(void)
{
   numbers data;	// struct variable data of type numbers

   data.num1 = 2;	// direct assignment of num1 with value 2
   data.num2 = 4;	// direct assignment of num2 with value 4

   printf("%d + %d = %d\n",data.num1, data.num2, (data.num1 + data.num2));

   return 0;
}
