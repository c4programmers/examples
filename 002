/* example 002 */
#include <stdio.h>
#include <math.h>

#define PI 3.141593

int main (void) 
{
   double degree, result, rad;

   degree = 60.0;           // angle in degree
   rad = PI / 180;          // conversion degree to radians
   result = sin(degree * rad);  // call the function sin with radians as parameter
   printf("The sine of %.2lf degree is %lf ", degree, result);
   return(0);
}
