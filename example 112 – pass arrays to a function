/* example 112 â€“ pass arrays to a function */
#include <stdio.h>

int calcSum(int arr[3][3]) {
    int sum = 0;

    for(int i=0; i<3; i++) {
        for(int j=0; j<3; j++) {
            printf("%d,",arr[i][j]);
            sum += arr[i][j];
        }
        printf("\n"); // next row
    }
    return sum;
}

int main(void)
{
    int sum;
    int myArray[3][3] = {
        {1,2,3},
        {4,5,6},
        {7,8,9},
    };

    sum = calcSum(myArray);
    printf("\nThe sum in total = %d",sum); 
    return 0;
}
