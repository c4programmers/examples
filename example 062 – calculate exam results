/* example 062 â€“ calculate exam results */
#include <stdio.h>

int main(void)
{
   int scoreTest1, scoreTest2;

   printf("Enter the score of the first test: ");
   scanf("%i",&scoreTest1);
   printf("Enter the score of the second test: ");
   scanf("%i",&scoreTest2);

   if (scoreTest1 > 16 && scoreTest2 > 16)
      printf("Gratulation, you passed!");

   else
      printf("Sorry, you failed!");

   return 0;
}
