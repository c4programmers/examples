/* example 014 - conversion of temperature fahrenheit in celsius */
#include <stdio.h>

double fahrenheit=100, celsius=0;  	 // global variables

int main(void)
{
   celsius = ((5.0/9.0)*(fahrenheit-32));  // Division as floatingpoint number!
   printf("%.0f degrees Fahrenheit = %.2f degrees Celsius\n",fahrenheit, celsius);
   return 0;
}
